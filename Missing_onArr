 int s= n*(n+1)/2;
        int sum=0;
        //Sum of the given array
        for(int i =0; i<n-1;i++){
            sum = sum + arr[i];
        }
        //return the missing element
        return s-sum;

//
int missingNumber(int n, int arr[]) {
        Arrays.sort(arr);
        for(int i=0;i<n-1;i++){
            if(arr[i]==i+1){continue;
            }
            else{
                return i+1;
            }
        }
    return n;
